{
  "expo": {
    "name": "shuttleTracker",
    "slug": "shuttle-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "fonts": "./assets/fonts/"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow TracSit to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e467026c-3e4e-4740-9bed-dc719e1d55ba"
      }
    },
    "owner": "mad_akwasi"
  }
}
